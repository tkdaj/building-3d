{
  "name": "building-3d",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pretty-quick:staged": "pretty-quick --staged",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\"",
    "check-format": "prettier --check \"./**/*.{ts,tsx,js,jsx,json}\"",
    "lint": "TIMING=1 DEBUG=eslint:cli-engine eslint --ext .js,.jsx,.tsx,.ts ./ --cache",
    "lint-ci": "npm run lint -- --max-warnings 0",
    "lint-fix": "npm run lint -- --fix",
    "tsc": "tsc --noEmit",
    "check-code": "run-p check-format lint-ci tsc --aggregate-output"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.7",
    "@types/node": "^18.11.9",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-implicit-dependencies": "^1.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "three": "^0.150.0"
  }
}
